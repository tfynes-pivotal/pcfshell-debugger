#!/bin/bash

set -euo pipefail

BUILD_DIR=$1
CACHE_DIR=$2
DEPS_DIR=$3
DEPS_IDX=$4

echo
echo "ROOT / DEFAULT FOLDER = " pwd && echo && ls -laR .
echo
echo "FIND shellinaboxd " && find / -name shellinaboxd
echo
echo
echo "BUILD = " && ls -laR $BUILD_DIR
echo
echo
echo "CACHE = " && ls -laR $CACHE_DIR
echo
echo
echo "DEPS = " && ls -laR $DEPS_DIR
echo
echo
#echo "DEPS-IDX = $DEPS-IDX"
echo
